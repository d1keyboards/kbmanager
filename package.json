{
	"name": "KBManager",
	"version": "1.0.0",
	"description": "D1 Keyboard Manager",
	"license": "GPL3",
	"private": false,
	"scripts": {
		"start": "concurrently -k npm:watch:app npm:watch:electron npm:start:electron",
		"eslint": "eslint ./app ./electron",
		"start:electron": "wait-on build/start.js && wait-on build/app/index.js && electron build/start.js",
		"tsc:app": "tsc --noEmit -p ./app/tsconfig.json",
		"tsc:electron": "tsc --noEmit -p ./electron/tsconfig.json",
		"watch:app": "webpack --config app/webpack.dev.js -o build/app --watch",
		"watch:electron": "tsc --outDir build -p ./electron/tsconfig.json --watch",
		"validate": "concurrently npm:tsc:app npm:tsc:electron npm:eslint npm:test:ci",
		"version": "mkdir -p public && echo \"{ \\\"version\\\": \\\"$(git describe --tags)\\\" }\" > public/version.json",
		"prepare": "husky install",
		"postinstall": "electron-rebuild",
		"test": "jest --watch",
		"test:ci": "jest --passWithNoTests"
	},
	"engines": {
		"node": ">=14.0.x"
	},
	"jest": {
		"moduleDirectories": [
			"node_modules",
			"app/src",
			"electron/src"
		],
		"moduleFileExtensions": [
			"js",
			"ts",
			"tsx"
		],
		"transform": {
			"\\.ts$": "babel-jest"
		}
	},
	"author": "d1keyboards.com",
	"devDependencies": {
		"@babel/core": "^7.15.0",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.14.7",
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@babel/preset-env": "^7.15.0",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.15.0",
		"@emotion/babel-plugin": "^11.3.0",
		"@emotion/babel-preset-css-prop": "^11.2.0",
		"@emotion/eslint-plugin": "^11.2.0",
		"@emotion/jest": "^11.3.0",
		"@types/lodash": "^4.14.172",
		"@types/node-hid": "^1.3.1",
		"@types/react": "^17.0.19",
		"@types/react-dom": "^17.0.9",
		"@types/react-redux": "^7.1.18",
		"@types/react-router-dom": "^5.1.8",
		"@typescript-eslint/eslint-plugin": "^4.29.2",
		"babel-eslint": "10.1.0",
		"babel-jest": "27.0.6",
		"babel-loader": "8.2.2",
		"babel-plugin-formatjs": "^10.3.5",
		"babel-plugin-graphql-tag": "^3.3.0",
		"babel-plugin-lodash": "3.3.4",
		"concurrently": "^6.2.1",
		"copy-webpack-plugin": "^9.0.1",
		"copyfiles": "^2.4.1",
		"css-loader": "^6.2.0",
		"css-minimizer-webpack-plugin": "^3.0.2",
		"electron": "^13.2.1",
		"electron-rebuild": "^3.1.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.24.0",
		"eslint-plugin-jest": "^24.4.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^7.0.0",
		"jest": "27.0.6",
		"prettier": "^2.3.2",
		"shx": "^0.3.3",
		"typescript": "^4.3.5",
		"wait-on": "^6.0.0",
		"webpack": "^5.51.1",
		"webpack-cli": "^4.8.0",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"@babel/runtime": "^7.15.3",
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11.3.0",
		"@ijprest/kle-serial": "github:d1keyboards/kle-serial#dayone-v1",
		"json5": "^2.2.0",
		"lodash": "^4.17.21",
		"node-hid": "^2.1.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-intl": "^5.20.9",
		"react-redux": "^7.2.4",
		"react-router-dom": "^5.2.0",
		"tslib": "^2.3.1",
		"usb-detection": "^4.10.0"
	}
}
